Ex:

<body>
    <ul>
        <li>gani</li>
        <li>ravi</li>
        <li>yash</li>
        </ul>
        <script>
        const ul=document.querySelector("ul")
      const  list=document.querySelectorAll("li")
      console.log(ul)
      console.log(list)
      for( const i of list){
        console.log(removing:${i.textContent})
        ul.removeChild(i)
        console.log(i)
      }
      console.log(list)
        </script>
